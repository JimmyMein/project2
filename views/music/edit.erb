<form action="/music/<%= music['id'] %>" method="POST">
        <input type="hidden" name="_method" value="put">
        
        <label for="">Artist </label>
        <input type="text" name='name' value="<%= music['name'] %>">

        <label for="">Song Name</label>
        <input type="text" name="sound_name" value="<%= music['sound_name'] %>">

        <label for="">Image </label>
        <input type="text" name="img_url" value="<%= music['img_url'] %>">
        
        <label for="">URL </label>
        <input type="text" name="url" value="<%= music['url'] %>">

        <button>Edit
        </button>
</form>
